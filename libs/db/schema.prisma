generator client {
  provider = "prisma-client"
  output   = "./prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model entry {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  content String?
  name    String
  parent  String? @db.ObjectId
  type    String
}
